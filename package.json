{
  "name": "blinkit",
  "description": "",
  "version": "0.1.0",
  "private": true,
  "repository": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint src",
    "prettier": "prettier --write '**/*.{js,json,css,md}'",
